{
  "manifest_version": 3,
  "name": "Bookmarks | NewTab",
  "version": "0.0.1",
  "description": "Bookmark organization and management in new tab.",
  "permissions": ["scripting", "tabs", "activeTab", "bookmarks"],
  "chrome_url_overrides": { "newtab": "index.html" },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
